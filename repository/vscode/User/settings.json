{
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "diffEditor.diffAlgorithm": "advanced",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.cursorBlinking": "solid",
    "editor.fontFamily": "Inconsolata, monospace",
    "editor.fontSize": 16,
    "editor.formatOnSave": true,
    "editor.lineHeight": 1.2,
    "editor.renderWhitespace": "trailing",
    "editor.suggestSelection": "first",
    "editor.tabSize": 2,
    "extensions.autoUpdate": "onlyEnabledExtensions",
    "extensions.ignoreRecommendations": true,
    "files.eol": "\n",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
    "java.recommendations.dependency.analytics.show": false,
    "json.schemaDownload.enable": false,
    "npm.fetchOnlinePackageInfo": false,
    "python.defaultInterpreterPath": "python3",
    "python.experiments.enabled": false,
    "python.formatting.provider": "yapf",
    "python.formatting.yapfArgs": [
        "--style={based_on_style: google, dedent_closing_brackets: true}"
    ],
    "python.formatting.yapfPath": "/opt/homebrew/bin/yapf",
    "python.pythonPath": "python3",
    "python.showStartPage": false,
    "rust.clippy_preference": "on",
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.emptyWindow": false,
    "security.workspace.trust.startupPrompt": "never",
    "security.workspace.trust.untrustedFiles": "newWindow",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.showExitAlert": false,
    "terminal.integrated.showLinkHover": false,
    "typescript.disableAutomaticTypeAcquisition": true,
    "vim.changeWordIncludesWhitespace": true,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "commands": [
                "redo"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "commands": [
                ":nohl",
            ]
        },
    ],
    "vim.visualstar": true,
    "vim.wrapscan": false,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.editor.enablePreview": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.wrapTabs": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.enableExperiments": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none",
}
